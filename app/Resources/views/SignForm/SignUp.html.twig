                            {#<div >#}
                                {#<div> <label class="form-element-label" >{[{item}]}</label></div>#}
                                {#<label class="form-element-label" >COUNTRY</label>#}

                            {#</div>#}


                            {#<ul class="country_list" style="width: 100%; height: 200px; overflow: auto">#}
                                {#<div ng-repeat="x in sign_up_countries">#}
                                    {#<li><img class="language-img" src='{{asset("img/square-flags/png/{[{x.img}]}.png")}}' alt="gb"> {[{x.name}]} ( {[{x.dial_code}]})</li>#}
                                {#</div>#}
                            {#</ul>#}
                        {#</div>#}

                    {#</div>#}

                    {#<div class="col-md-3 col-sm-3 col-xs-2 form-element form-input" style="padding-left: 0;padding-right: 0; margin-top: 0;margin-bottom: 40px;">#}
