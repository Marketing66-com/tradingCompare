            .burger {
                float: right;
                cursor: pointer;
                width: 40px;
                height: 30px;
                margin-top: 17px;
            }

            .burger .burger-menu {
                width: 100%;
                height: 2px;
                background-color: white;
                -webkit-transition: all 0.3s;
                -o-transition: all 0.3s;
                transition: all 0.3s;
                float: right;
            }

            .burger .burger-menu:nth-child(2) {
                width: 100%;
                -webkit-transition-property: margin, height, width;
                -o-transition-property: margin, height, width;
                transition-property: margin, height, width;
                -webkit-transition-delay: 0.3s, 0.3s, 0s;
                -o-transition-delay: 0.3s, 0.3s, 0s;
                transition-delay: 0.3s, 0.3s, 0s;
                margin: 10px 0;
            }

            .burger .burger-menu:last-child {
                width: 100%;
            }

            .burger:hover .burger-menu {
                background-color: white;
            }

            .burger:hover .burger-menu:nth-child(2) {
                width: 100%;
                -webkit-transition-delay: 0.3s, 0.3s, 0s;
                -o-transition-delay: 0.3s, 0.3s, 0s;
                transition-delay: 0.3s, 0.3s, 0s;
            }

            .burger:hover .burger-menu:last-child {
                width: 100%;
            }

            .burger.opened {
                padding-top: 12px;
            }

            .burger.opened .burger-menu {
                float: none;
            }

            .burger.opened .burger-menu:nth-child(1) {
                -webkit-transition-delay: 0.3s;
                -o-transition-delay: 0.3s;
                transition-delay: 0.3s;
                height: 3px;
                -webkit-transform: rotate(45deg);
                -ms-transform: rotate(45deg);
                -o-transform: rotate(45deg);
                transform: rotate(45deg);
                width: 100%;
            }

            .burger.opened .burger-menu:nth-child(2) {
                opacity: 0;
                height: 0;
                margin: -3px;
                width: 100%;
            }

            .burger.opened .burger-menu:last-child {
                -webkit-transition-delay: 0.3s;
                -o-transition-delay: 0.3s;
                transition-delay: 0.3s;
                height: 3px;
                -webkit-transform: rotate(-45deg);
                -ms-transform: rotate(-45deg);
                -o-transform: rotate(-45deg);
                transform: rotate(-45deg);
                width: 100%;
            }

