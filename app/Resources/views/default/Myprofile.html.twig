                                                <div class="review_list oh_no_signin">
                                                    {% include 'default/title_bar_list_feed.html.twig' %}

                                                    {#----> watchlist exist#}
                                                    <div class="review_list_cont-heder  ng-cloak" ng-repeat="item in WatchlistTable" >
                                                        <div class="abs">
                                                            <div class="general_review_cont-block review_list_name">
                                                                <div class="review_list_center">
