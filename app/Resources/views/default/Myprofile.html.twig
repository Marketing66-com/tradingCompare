                                                                    <img class="live_block_top_img sm-none" ng-src="{[{item.img}]}" onerror="this.src='/img/Stock_Logos/stocks.png'";>
                                                                    <h2 class="live_block_top_title">{[{item.name}]}</h2>
                                                                    <p class="live_block_money-right live_block_money_red live_block_money_border mobile_padding mobile-block mobile-block">{[{item.change24}]}</p>
                                                                </div>
                                                            </div>
                                                            <div class="general_review_cont-block review_list_cont-text">
                                                                <div class="review_list_center">
                                                                    <p class="live_block_money-right  mobile_margin"><span class="live_block_money_bold" >{[{item.price}]}</span></p>
                                                                </div>
                                                            </div>
                                                            <div class="general_review_cont-block review_list_cont-text sm-none">
                                                                <div class="review_list_center">
                                                                    <p class="live_block_money-right" ng-class="{'live_block_money_red live_block_money_border': item.change24 < 0, 'live_block_money_green live_block_money_border_green': item.change24 >= 0}">{[{item.change24}]}</p>
                                                                    {#<p class="live_block_money-right live_block_money_green live_block_money_border_green" >{[{item.change24}]}</p>#}
                                                                </div>
                                                            </div>
                                                            <div class="general_review_cont-block review_list_cont-text sm-none">
                                                                <div class="review_list_center">
                                                                    <p class="live_block_money-right"><span class="live_block_money_bold">{[{item.high24}]}</span>
                                                                </div>
                                                            </div>
                                                            <div class="general_review_cont-block review_list_cont-text">
                                                                <div class="review_list_center">
                                                                    <p class="live_block_money-right  mobile_bold_margin"><span class="live_block_money_bold sm-none">{[{item.low24}]}</span>
                                                                        <span class="live_block_money_bold mobile-block">{[{item.low24}]}<br></span>
                                                                        <span class="live_block_money_bold mobile-block">/{[{item.high24}]}</span></p>
                                                                </div>
                                                            </div>

                                                            <div class="general_review_cont-block review_list_cont-bar ms-none">
                                                                <div class="review_list_center">
                                                                    {#SENTIMENT#}
                                                                    <p>
                                                                        {# logIn - Close #}
                                                                        <span ng-if="item.status_sentiment == 'CLOSE'" ng-click="add_sentiment($index, 'BULLISH')" class="chart_bitcoin-reting_prozent_like">
                                                                            <i  class="button_sentiment_list button_bullish_list" target="_blank"><b>{{ 'button.bullish'|trans }}</b></i>
                                                                        </span>
                                                                        <span ng-if="item.status_sentiment == 'CLOSE'" ng-click="add_sentiment($index, 'BEARISH')" class="chart_bitcoin-reting_prozent_like">
                                                                            <i class="button_sentiment_list button_bearish_list" target="_blank"><b>{{ 'button.bearish'|trans }}</b></i>
                                                                        </span>

                                                                        {# logIn - Open on Bullish #}
                                                                        <span ng-if="item.status_sentiment == 'OPEN' && item.type_sentiment == 'BULLISH'" ng-click="AlreadyOpen($index)" class="chart_bitcoin-reting_prozent_like">
                                                                            <i  class="button_sentiment_list button_bullish_list_selected" target="_blank"><b>{{ 'button.bullish'|trans }}</b></i>
                                                                        </span>
                                                                        <span ng-if="item.status_sentiment == 'OPEN' && item.type_sentiment != 'BEARISH'" ng-click="AlreadyOpen($index)" class="chart_bitcoin-reting_prozent_like">
                                                                            <i  class="button_sentiment_list button_bearish_list" target="_blank"><b>{{ 'button.bearish'|trans }}</b></i>
                                                                        </span>

                                                                        {# logIn - Open on Bearish #}
                                                                        <span ng-if="item.status_sentiment == 'OPEN' && item.type_sentiment != 'BULLISH'" ng-click="AlreadyOpen($index)" class="chart_bitcoin-reting_prozent_like">
                                                                            <i class="button_sentiment_list button_bullish_list" target="_blank"><b>{{ 'button.bullish'|trans }}</b></i>
                                                                        </span>
                                                                        <span ng-if="item.status_sentiment == 'OPEN' && item.type_sentiment == 'BEARISH'" ng-click="AlreadyOpen($index)" class="chart_bitcoin-reting_prozent_like">
                                                                            <i class="button_sentiment_list button_bearish_list_selected" target="_blank"><b>{{ 'button.bearish'|trans }}</b></i>
                                                                        </span>
                                                                    </p>

                                                                    <div class="chart_bitcoin-reting_progress-bar">

                                                                        <div ng-if="item.more_bullish" class="reting_progress-bar_click chart_bitcoin-reting_prozent" >{[{item.sentiment}]}% Bullish</div>
                                                                        <div ng-if="!item.more_bullish" class="reting_progress-bar_click chart_bitcoin-reting_prozent reting_progress-bar_click_red" >{[{item.sentiment}]}% Bearish</div>
                                                                        {#<div class="reting_progress-bar_click chart_bitcoin-reting_prozent reting_progress-bar_click_red" >54% Bearish</div>#}
                                                                    </div>
                                                                </div>
                                                            </div>

                                                            <div class="general_review_cont-block review_list_cont-chart">
                                                                <div class="review_list_center">
                                                                    <a ng-click="ActiveChange_Watchlist(item.pair, item.type, item.complete_name,'{{ app.request.getLocale() }}');"><img class="live_block_chart" src='{{asset("img/chart-design/chart_1.png")}}'  alt="chart_1"></a>
                                                                </div>
                                                            </div>

                                                            <div class="general_review_cont-block review_list_cont-btn">
                                                                <div class="review_list_center">
                                                                    <a class="live_trade-btn" href="http://borthy-carges.com/98dd47bc-5797-47bf-847a-784a2445ee36" target="_blank">{{ 'button.trade'|trans }} <span class="live_trade-btn_none">{{ 'button.now'|trans }}</span></a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div style="height: 50px;"></div>
                                                </div>
                                            </div>
                                        </div>

                                        {#/************************************************************************************/#}
                                        <div ng-show="isSet(3)">
                                            <hr >
                                            <div>
                                                <div class="post_user-cont each_post" ng-repeat="item in get_following" >
                                                    <a class="post_news" ng-click="clickid(item._id)" target="_blank">

                                                        <div class="row">
                                                            <div class="col-md-1 col-sm-1 col-xs-2">
                                                                <div class="post-cont_img">
                                                                    <img class="post_user-img" ng-src="{{ asset("img/Flags1/{[{item.mycountry}]}.png")}}" alt={[{item.country}]}>
                                                                </div>
                                                            </div>

                                                            <div class="col-md-8 col-sm-8 col-xs-4">
                                                                <div class="post_user-cont_text">
                                                                    <p class="post_user-cont_name">{[{item.nickname}]}</p>
                                                                </div>
                                                            </div>

                                                            <div class="col-md-3 col-sm-3 col-xs-6">
                                                                <a ng-if="item.myFollow" ng-click="remove_follow($index)" class="following-btn sm-none" style="">FOLLOWING</a>
                                                                <a ng-if="!item.myFollow" ng-click="add_follow($index)" class="follow-btn follow_in_table sm-none" style="">FOLLOW</a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                </div>

                                                {#<div class="review_cont-btn">#}
                                                {#<a class="review_more-btn" ng-class="nextPageDisabledClass()" ng-style="disabled"#}
                                                {#ng-click="loadMore()" >MORE TWEETS</a>#}
                                                {#</div>#}
                                            </div>

                                            {#<div ng-if="">#}
                                            {#<p class="news_space_absolut_text">No tweet for {{ symbol }}</p>#}
                                            {#</div>#}
                                        </div>

                                        {#/************************************************************************************/#}
                                        <div ng-show="isSet(4)">
                                            <hr>
                                            <div>
                                                <div class="post_user-cont each_post" ng-repeat="item in get_followers">
                                                    <a class="post_news" ng-click="clickid(item._id)"  target="_blank">

                                                        <div class="row">
                                                            <div class="col-md-1 col-sm-1 col-xs-2">
                                                                <div class="post-cont_img">
                                                                    <img class="post_user-img" src='{{ asset("img/Flags1/{[{item.mycountry}]}.png")}}' alt={[{item.country}]}>
                                                                </div>
                                                            </div>

                                                            <div class="col-md-8 col-sm-8 col-xs-4">
                                                                <div class="post_user-cont_text">
                                                                    <p class="post_user-cont_name">{[{item.nickname}]}</p>
                                                                </div>
                                                            </div>

                                                            <div class="col-md-3 col-sm-3 col-xs-6">
                                                                <a ng-if="item.myFollow" ng-click="remove_follow_from_followers($index)"  class="following-btn sm-none" style="">FOLLOWING</a>
                                                                <a  ng-if="!item.myFollow" ng-click="add_follow_from_followers($index)"  class="follow-btn follow_in_table sm-none" style="">FOLLOW</a>
                                                            </div>
                                                        </div>
                                                    </a>
                                                </div>

                                                {#<div class="review_cont-btn">#}
                                                {#<a class="review_more-btn" ng-class="nextPageDisabledClass()" ng-style="disabled"#}
                                                {#ng-click="loadMore()" >MORE TWEETS</a>#}
                                                {#</div>#}
